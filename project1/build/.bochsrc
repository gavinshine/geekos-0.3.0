# An example .bochsrc file.

# You will need to edit these lines to reflect your system.
vgaromimage: file=/usr/local/bochs/share/bochs/VGABIOS-lgpl-latest
romimage: file=/usr/local/bochs/share/bochs/BIOS-bochs-latest

megs: 8
boot: a

# diskc: path=diskc.img, cyl=40, heads=8, spt=64
ata0-master: type=disk, path=diskc.img, mode=flat, cylinders=40, heads=8, spt=63, translation=none

floppya: 1_44=fd.img, status=inserted
#floppya: 1_44=fd_aug.img, status=inserted

log: ./bochs.out

private_colormap: enabled=0



